
public class Student {
	
	private String name;
	private double GPA;
	
	public Student (String name, double GPA) {
		this.name = name;
		this.GPA = GPA;
	}
	
	public String getName() {
		return this.name;
	}
	
	public void setName(String newName) {
		this.name = newName;
	}
	
	public double getGPA() {
		return this.GPA;
	}
	
	public void changeGPA(double newGPA) {
		this.GPA = newGPA;
	}
	
	
}
