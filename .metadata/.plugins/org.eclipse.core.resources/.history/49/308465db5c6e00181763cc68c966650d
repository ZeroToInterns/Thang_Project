
public class Tester {
	public static void main(String[] args) {
		ControlFlowPractice testing = new ControlFlowPractice();
		System.out.println(testing.calculateCost(51));
		System.out.println(testing.calculateCost(35));
		System.out.println(testing.calculateCost(6));
		testing.rollDice(1);
		testing.rollDice(2);
		testing.rollDice(3);
		testing.rollDice(4);
		testing.rollDice(5);
		testing.rollDice(6);
		System.out.println(testing.findLetterAt(1, "Hello World"));
	}
}
